{
  "name": "parabank-cypress",
  "version": "1.0.0",
  "main": "cypress.config.js",
  "scripts": {
    "cypress:run": "cypress run --headless --reporter mochawesome --reporter-options overwrite=false,html=true,json=true,reportDir=cypress/reports/mochawesome",
    "merge-report": "mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochawesome/mochawesome.json",
    "generate-report": "marge cypress/reports/mochawesome/mochawesome.json --reportDir cypress/reports/final --inline --assets-inline"
  },

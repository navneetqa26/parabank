  "license": "ISC",
  "description": "",
  "dependencies": {
    "cypress": "^15.0.0"
  },
  "devDependencies": {
    "cypress-mochawesome-reporter": "^3.8.4",
    "dotenv": "^17.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.2.0"
  }
}

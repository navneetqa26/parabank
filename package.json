  "scripts": {
    "test": "cypress run",
    "merge:reports": "if ls cypress/reports/*.json 1> /dev/null 2>&1; then mochawesome-merge cypress/reports/*.json > cypress/reports/merged.json; else echo '{\"stats\":{},\"results\":[]}' > cypress/reports/merged.json; fi",
